#include<stdio.h>
int main ()
{
    int a[100],n,k,i,found=0;
    printf("Enter the element: \n");
    scanf("%d",&n);
    printf("Enter %d element: \n");
    for(i=0;i<n;i++){
    scanf("%d",&a[i]);
    }
    printf("Enter the element to search:\n");
    scanf("%d",&k);
    for(i=0;i<n;i++){
        if(a[i]==k){
            printf("Element found at position : %d \n",i+1);
            found=1;
            break;
        }
    }
    if(found==0){
    printf("Element not found \n");
}
return 0;
}
