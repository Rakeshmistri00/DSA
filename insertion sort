#include<stdio.h>
int main()
{
    int a[100],i,j,k,n;
    printf("Enter the no of Element:\n");
    scanf("%d",&n);
    printf("Enter element:\n");
    for(i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
    for(i=1;i<n;i++){
        k=a[i];
        j=i-1;
        while(j>=0 && a[j]>k){
            a[j+1]=a[j];
            j--;
        }
        a[j+1]=k;
    }
    printf("sorted array:\n");
    for(i=0;i<n;i++)
    printf("%d \t",a[i]);
    return 0;
}
