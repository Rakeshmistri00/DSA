#include<stdio.h>
int main ()
{
    int a[100], i, n, pos, element;

    printf("Enter the no. of elements: ");
    scanf("%d",&n);
    printf("Enter array elements:\n");
    for(i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
    printf("Enter position to insert (1 to %d): ", n);
    scanf("%d",&pos);
    printf("Enter element to insert: ");
    scanf("%d",&element);
    for(i=n; i>=pos; i--){
        a[i] = a[i-1];
    }

    a[pos-1] = element;
    n++;
    printf("Array after insertion:\n");
    for(i=0;i<n;i++){
        printf("%d ",a[i]);
    }
    return 0;
}
